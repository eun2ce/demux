{
   "compilerOptions":{
      "baseUrl": "src",
      "paths":{
      },
      "plugins":[
         { "name": "typescript-tslint-plugin" }
      ],
      "resolveJsonModule": true,
      "module": "commonjs",
      "outDir": "./dist",
      "allowJs": true,
      "target": "es2017",
      "allowSyntheticDefaultImports": true
   },
   "include": [
      "./src/**/*"
   ],
   "exclude":[
      "node_modules"
   ]
}
